prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=@libdir_for_pc_file@
includedir=@includedir_for_pc_file@

Name: rayscene
Description: A scene manager for raylib.
URL: https://github.com/SomeDudeNamedAnthony/rayscene
Version: @PROJECT_VERSION@
Libs: -L"${libdir}" -lrayscene @PKG_CONFIG_LIBS_EXTRA@
Libs.private: @PKG_CONFIG_LIBS_PRIVATE@
Requires.private: @GLFW_PKG_DEPS@
Cflags: -I"${includedir}"
